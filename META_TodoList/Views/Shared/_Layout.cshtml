@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<!DOCTYPE html>
<html lang ="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@500&display=swap" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Alkatra&family=Playfair+Display:wght@700&display=swap"
          rel="stylesheet">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Playfair+Display:wght@700&display=swap" rel="stylesheet">
    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@40,700,1,200" />
    <link rel="stylesheet" href="~/css/UserSignIn.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/Layout.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/other.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ToDoListWebApplication.styles.css" asp-append-version="true" />

    @await RenderSectionAsync("HeaderTags", required: false)
</head>
<body>
    <header class="head">
        <a href="https://meta.vn/">
@*            <img src="data:image/png;base64,"
                 alt="logo" class="logo">*@
            <img src="data:image/png;base64,"
                alt="logo" class="logo">
        </a>
        <ul class="main-nav">
            <li><a asp-area="" asp-controller="Home" asp-action="HowClientThink"> How Client Think </a></li>
            <li><a href="https://meta.vn/gioithieu"> About Us </a></li>
            <li><a asp-area="" asp-controller="Home" asp-action="Product"> Products </a></li>
            <li><a asp-area="" asp-controller="Home" asp-action="Contact"> Contact </a></li>
        </ul>
        <div class="clearfix"></div>
        <div class="row">
            <br>
            <h1 class="heading-main-text">
                To do list online .
                <br>
                Join with us !
            </h1>
            <p class="buttonbtn">
            <a class="nav-link" asp-page="/Error">Tell me more</a>
            </p>
            <p class="buttonbtn">
            <a class="nav-link" asp-area="" asp-controller="User" asp-action="SignIn">Join Us</a>
            </p>
        </div>
    </header>
    <div class="container">
        <main>
            @RenderBody()
        </main>
    </div>
    <footer class="about">
        <h2 style="color: rgb(248, 74, 74);">About Us</h2>
        <p style="color: black" class="note">
            Our product help you to note your work , complete your work quickly
            <br>
            This product create by ToannDinhh .
        </p>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/API-URL.js" asp-append-version="true"></script>
    <script src="~/js/ValidatorSignUp.js" asp-append-version="true"></script>
    <script src="~/js/ValidatorSignIn.js" asp-append-version="true"></script>
    <script src="~/js/ValidatorData.js" asp-append-version="true"></script>
    <script src="~/js/CheckUser.js"asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
